// Ambil nama blog dari URL
var blogName = window.location.hostname;

// Ganti dengan nama blog Anda yang sesuai
var validBlogName = "tekno-inpodia.blogspot.com";

// Jika nama blog tidak sama dengan nama blog yang valid, maka redirect ke URL lain
if (blogName !== validBlogName) {
  window.location.replace("https://www.inpodia.com");
}
